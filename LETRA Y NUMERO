# Pedir al usuario que ingrese un carácter
caracter = input("caracter: ")

# Verificar si es un número
if caracter.isdigit():
    print("Es número")
else:
    # Obtener el valor entero del carácter en el código ASCII
    valor_ascii = ord(caracter)

    # Verificar si es una letra mayúscula
    if 65 <= valor_ascii <= 90:
        print("Es letra mayúscula")
    # Verificar si es una letra minúscula
    elif 97 <= valor_ascii <= 122:
        print("Es letra minúscula")
    else:
        print("No es letra ni número")
